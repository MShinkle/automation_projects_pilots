{
  "output": {
    "filePath": "code_summary.txt",
    "style": "plain",
    "compress": false,
    "removeComments": false,
    "fileSummary": true,
    "directoryStructure": true,
    "removeEmptyLines": true,
    "topFilesLength": 5,
    "showLineNumbers": false,
    "includeEmptyDirectories": true,
    "headerText": "This file is a consolidated single-file compilation of all code in the repository generated by Repomix. Note that .ipynb files have been converted to .py files."
  },
  "include": [
    "**/*.py",
    "**/*.md",
    "**/*.txt"
  ],
  "ignore": {
    "useGitignore": false,
    "useDefaultPatterns": false,
    "customPatterns": [
      "**/.git/**",
      "**/.github/**"
    ]
  },
  "security": {
    "enableSecurityCheck": true
  },
  "tokenCount": {
    "encoding": "o200k_base"
  }
}
